<!DOCTYPE html>
<html>
<head>
  <title>PPatch</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <header>
    <% if session[:user_id] %>
      <%= link_to "Sign Out", logout_path %>
    <% else %>
      <%= link_to "Sign Up", new_user_path %>
      <%= link_to "Sign In", "#" %>
    <% end %>
  </header>

<%= yield %>

</body>
</html>
